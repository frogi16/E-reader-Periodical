<rules>
	<rule>
		<domain>global</domain>
		<minWords>100</minWords>
		<maxWords>0</maxWords>
		<XMLFilteringRules>
		</XMLFilteringRules>
	</rule>
	<rule>
		<domain>www.imperiumromanum.edu.pl</domain>
		<minWords>200</minWords>
		<maxWords></maxWords>
		<XMLFilteringRules>
			<XMLRule>
				<type>attribute-value</type>
				<attribute>id</attribute>
				<value>wbounce-config</value>
			</XMLRule>
			<XMLRule>
				<type>attribute-value</type>
				<attribute>class</attribute>
				<value>inner</value>
			</XMLRule>
			<XMLRule>
				<type>attribute-value</type>
				<attribute>class</attribute>
				<value>amphtml-content</value>
			</XMLRule>
			<XMLRule>
				<type>attribute-value</type>
				<attribute>class</attribute>
				<value>dol-strony</value>
			</XMLRule>
			<XMLRule>
				<type>attribute-value</type>
				<attribute>class</attribute>
				<value>okruszki</value>
			</XMLRule>
		</XMLFilteringRules>
	</rule>
	
	<rule>
		<domain>nowakonfederacja.pl</domain>
		<minWords>0</minWords>
		<maxWords></maxWords>
		<XMLFilteringRules>
			<XMLRule>
				<type>text-substring</type>
				<substring>Nowa Konfederacja istnieje dzięki naszym
Darczyńcom</substring>
			</XMLRule>
			<XMLRule>
				<type>text-substring</type>
				<substring>Wsparcie co miesiąc</substring>
			</XMLRule>
		</XMLFilteringRules>
	</rule>
	
	<rule>
		<domain>niebezpiecznik.pl</domain>
		<minWords>0</minWords>
		<maxWords></maxWords>
		<XMLFilteringRules>
			<XMLRule>
				<type>attribute-value</type>
				<attribute>class</attribute>
				<value>date</value>
			</XMLRule>
		</XMLFilteringRules>
	</rule>
	
	<rule>
		<domain>www.kwantowo.pl</domain>
		<minWords>0</minWords>
		<maxWords></maxWords>
		<XMLFilteringRules>
			<XMLRule>
				<type>attribute-value</type>
				<attribute>class</attribute>
				<value>top-bar top-bar-overlay top-bar-sticky</value>
			</XMLRule>
			<XMLRule>
				<type>attribute-value</type>
				<attribute>class</attribute>
				<value>page-sidebar</value>
			</XMLRule>
		</XMLFilteringRules>
	</rule>
	
	<rule>
		<domain>www.totylkoteoria.pl</domain>
		<minWords>0</minWords>
		<maxWords></maxWords>
		<XMLFilteringRules>
			<XMLRule>
				<type>text-substring</type>
				<substring>Prowadzenie bloga naukowego wymaga ponoszenia kosztów.</substring>
			</XMLRule>
			<XMLRule>
				<type>text-substring</type>
				<substring>Patronite</substring>
			</XMLRule>
			<XMLRule>
				<type>text-substring</type>
				<substring>gdzie w
prosty sposób możecie</substring>
			</XMLRule>
			<XMLRule>
				<type>attribute-value</type>
				<attribute>class</attribute>
				<value>separator</value>
			</XMLRule>
		</XMLFilteringRules>
	</rule>
</rules>